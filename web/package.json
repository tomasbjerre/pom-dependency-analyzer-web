{
  "name": "pom-dependency-analyzer-web",
  "version": "1.0.0",
  "private": true,
  "author": "",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm install --verbose && npm run generate-client && npm run format && vue-cli-service build",
    "lint": "vue-cli-service lint --fix",
    "format": "./node_modules/.bin/prettier --single-quote --write '{public,src}/**/*.!(sh|png|*ignore|ico|svg|*pts)' --write '*.?(json|js)'",
    "generate-client": "./node_modules/.bin/openapi-generator generate -g typescript-axios -i ../swagger.yml -o src/services/pdaw --additional-properties supportsES6=true"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.3.5",
    "core-js": "^2.6.5",
    "material-design-icons-iconfont": "^3.0.3",
    "request": "^2.72.0",
    "roboto-fontface": "*",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vuetify": "^1.5.5",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "0.0.9-4.0.0-beta3",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-typescript": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "prettier": "1.17.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "typescript": "^3.4.3",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.5.21",
    "vuetify-loader": "^1.0.5",
    "typings": "^1.3.0"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bugs": {
    "url": "https://github.com/tomasbjerre/pom-dependency-web/issues"
  },
  "homepage": "https://github.com/tomasbjerre/pom-dependency-web#readme",
  "license": "Apache 2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomasbjerre/pom-dependency-web.git"
  }
}
